@using MySocialNetwork.DTO
@model IEnumerable<MySocialNetwork.DTO.GroupDto>
<div>
    <div>
        @if (Model.Count() > 0)
        {
            foreach (GroupDto groupDto in Model)
            {
                <p>
                    @Html.ActionLink(groupDto.Title, "ShowGroupPage", "Page", new {groupId = groupDto.Id}, null);
                </p>
            }
        }
        else
        {
            <label>Sorry, no groups</label>
        }
    </div>
</div>

